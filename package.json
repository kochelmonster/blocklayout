{
  "name": "blocklayout",
  "version": "1.0.0",
  "description": "A html preprocessor to transform a simple block graphic into a flex grid.",
  "exports": {
    "./svelte": "./dist/svelte.js"
  },
  "files": [
    "dist/",
    "bin/",
    "examples/"
  ],
  "type": "module",
  "bin": {
    "blocklayout": "./bin/blocklayout.js"
  },
  "scripts": {
    "prepublishOnly": "coffee --compile --output dist src",
    "convert": "coffee --compile --output dist src"
  },
  "keywords": [
    "design",
    "prototyping",
    "layout",
    "library",
    "html",
    "preprocessor"
  ],
  "homepage": "https://github.com/kochelmonster/blocklayout",
  "author": "kochelmonster",
  "license": "MIT",
  "devDependencies": {
    "coffeescript": "^2.7.0"
  },
  "dependencies": {
    "jsdom": "^24.1.0"
  }
}